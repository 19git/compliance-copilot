# Compliance Copilot Environment Variables
# Copy to .env and fill in your values

# Environment
COMPLIANCE_ENV=development
COMPLIANCE_DEBUG=true

# Authentication
COMPLIANCE_API_KEY=your-secure-api-key-here

# Database
COMPLIANCE_DB_PATH=compliance.db

# Logging
COMPLIANCE_LOG_LEVEL=INFO
COMPLIANCE_LOG_DIR=logs

# Cache
COMPLIANCE_CACHE_DIR=cache
COMPLIANCE_CACHE_TTL=3600
